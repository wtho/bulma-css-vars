  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/css": "0.0.31",
    "@types/jest": "^26.0.14",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^15.12.2",
    "@types/sass": "^1.16.0",
